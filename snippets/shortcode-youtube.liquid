{%- capture my_video_link -%}{% include 'shortcode-render' render:'video' default:'_9VUPq3SxOc' %}{%- endcapture -%}
{%- assign my_video = my_video_link | split: '?v=' -%}
<iframe id="ytplayer" type="text/html" width="{% include 'shortcode-render' render:'width' default:'640' %}" height="{% include 'shortcode-render' render:'height' default:'390' %}" src="//www.youtube.com/embed/{{ my_video | last }}" frameborder="0" ></iframe>