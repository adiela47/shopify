{%- include 'google-fonts' -%}
<script>
  var theme = {
    apps: {
      details: {{ 'apps.lookbook.details' | t | json }},
      buyNow: {{ 'apps.lookbook.buy_now' | t | json }},
    },
    strings: {
      addToCart: {{ 'products.product.add_to_cart' | t | json }},
      soldOut: {{ 'products.product.sold_out' | t | json }},
      unavailable: {{ 'products.product.unavailable' | t | json }},
      inStock: {{ 'products.product.in_stock' | t | json }},
      outStock: {{ 'products.product.out_of_stock' | t | json }},
      sale: {{ 'products.general.sale' | t | json }}
    },
    moneyFormat: {{ shop.money_format | json }},
    sale_percentages: {{ settings.shop_sale_percentages | json }},
    cart_ajax: {{ settings.product_addtocart_ajax | json }},
    asset_url: {{ '?' | asset_url | json }},
    popup_mailchimp_period: {{ settings.popup_mailchimp_period | plus:0 }},
    popup_mailchimp_delay: {{ settings.popup_mailchimp_delay | plus:0 }},
    compare: {{ settings.compare_enabled | plus:0 }},
    compare_limit: {{ settings.compare_limit | plus:0 }}
  }
  document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
</script>
